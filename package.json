{
  "name": "my-walking-app",
  "version": "1.0.0",
  "description": "A simple landing page made with Eleventy",
  "main": "index.js",
  "scripts": {
    "eleventy": "npx eleventy",
    "sass": "sass --style=compressed src/style/main.scss src/main.css",
    "watch:sass": "npm run sass -- --watch",
    "watch:eleventy": "npx eleventy --serve",
    "start": "npm-run-all sass --parallel watch:*",
    "build": "npm run eleventy && npm run sass"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pgmgent-2021-uip/exercises-adriaanglibert.git"
  },
  "keywords": [
    "Walking",
    "Eleventy"
  ],
  "author": "Adriaan Glibert",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pgmgent-2021-uip/exercises-adriaanglibert/issues"
  },
  "homepage": "https://github.com/pgmgent-2021-uip/exercises-adriaanglibert#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@11ty/eleventy-navigation": "^0.1.6",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {}
}
